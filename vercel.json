{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "ALLOWED_ORIGINS": "*"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 10
    }
  }
}
